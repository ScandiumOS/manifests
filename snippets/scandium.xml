  <!-- OTA -->
  <project path="OTA" name="OTA" remote="ScandiumOS-Devices">
      <linkfile src="ota.sh" dest="ota.sh" />
      <linkfile src="devices.json" dest="devices.json" />
  </project>
  
</manifest>
